# {{ansible_managed}}

apiVersion: 1

providers:
  - name: 'Provisioned Dashboards'
    orgId: 1
    folder: 'provisioned'
    type: file
    disableDeletion: true
    editable: false
    updateIntervalSeconds: 60
    allowUiUpdates: false
    options:
      path: '{{grafana_dashboard_dir}}'
      foldersFromFilesStructure: true
